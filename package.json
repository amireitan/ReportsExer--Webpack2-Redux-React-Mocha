{
  "name": "webpack2-react-redux",
  "version": "1.0.0",
  "description": "Webpack2, React, Redux, mocha",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all clean:tmp clean:dist dev --parallel  server test:watch lint:watch",
    "server": "webpack-dev-server",
    "dev": "cross-env NODE_ENV=development --env.prod=false",
    "pro": "cross-env NODE_ENV=production webpack-dev-server --env.prod=true",
    "build": "cross-env NODE_ENV=production webpack --env.prod=true",
    "prebuild:js": "clean:dist && npm run lint && npm run test",
    "lint": "node_modules/.bin/esw --color webpack.config.* src -f stylish true",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test  mocha-webpack --growl --colors --webpack-config webpack.config-test.js  \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean:dist": "rimraf dist",
    "clean:tmp": "rimraf .tmp"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "6.16.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-class-properties": "6.22.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "^6.23.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.14.5",
    "enzyme": "^2.7.1",
    "es6-promise": "^4.0.5",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "eslint-watch": "^2.1.14",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "0.8.5",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "nock": "^9.0.6",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "postcss": "^5.2.13",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^1.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-custom-scrollbars": "^4.0.2",
    "redux-logger": "^2.8.1",
    "redux-mock-store": "^1.2.2",
    "sass-loader": "^6.0.0",
    "source-map-support": "^0.4.11",
    "style-loader": "0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0",
    "webpack-hot-middleware": "^2.17.0",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "babel-runtime": "6.22.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0"
  }
}
